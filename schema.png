digraph G {
prog=dot;
mode=ipsep;
overlap=ipsep;
sep=0.01;
concentrate=True;
rankdir=TB;
compra [shape=plaintext, label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0"><TR><TD ALIGN="CENTER">compra</TD></TR><TR><TD BORDER="1" CELLPADDING="0"></TD></TR><TR><TD ALIGN="LEFT" PORT="id">- id : INTEGER</TD></TR><TR><TD ALIGN="LEFT" PORT="fornecedor_id">- fornecedor_id : INTEGER</TD></TR><TR><TD ALIGN="LEFT" PORT="data">- data : DATETIME</TD></TR></TABLE>>, fontname="Bitstream-Vera Sans", fontsize=7.0];
fornecedor [shape=plaintext, label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0"><TR><TD ALIGN="CENTER">fornecedor</TD></TR><TR><TD BORDER="1" CELLPADDING="0"></TD></TR><TR><TD ALIGN="LEFT" PORT="id">- id : INTEGER</TD></TR><TR><TD ALIGN="LEFT" PORT="nome">- nome : VARCHAR(100)</TD></TR><TR><TD ALIGN="LEFT" PORT="endereco">- endereco : VARCHAR(100)</TD></TR></TABLE>>, fontname="Bitstream-Vera Sans", fontsize=7.0];
compra_produto [shape=plaintext, label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0"><TR><TD ALIGN="CENTER">compra_produto</TD></TR><TR><TD BORDER="1" CELLPADDING="0"></TD></TR><TR><TD ALIGN="LEFT" PORT="id">- id : INTEGER</TD></TR><TR><TD ALIGN="LEFT" PORT="compra_id">- compra_id : INTEGER</TD></TR><TR><TD ALIGN="LEFT" PORT="produto_id">- produto_id : INTEGER</TD></TR><TR><TD ALIGN="LEFT" PORT="quantidade">- quantidade : INTEGER</TD></TR><TR><TD ALIGN="LEFT" PORT="preco_unitario">- preco_unitario : FLOAT</TD></TR></TABLE>>, fontname="Bitstream-Vera Sans", fontsize=7.0];
produto [shape=plaintext, label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0"><TR><TD ALIGN="CENTER">produto</TD></TR><TR><TD BORDER="1" CELLPADDING="0"></TD></TR><TR><TD ALIGN="LEFT" PORT="id">- id : INTEGER</TD></TR><TR><TD ALIGN="LEFT" PORT="nome">- nome : VARCHAR(100)</TD></TR><TR><TD ALIGN="LEFT" PORT="marca">- marca : VARCHAR(100)</TD></TR></TABLE>>, fontname="Bitstream-Vera Sans", fontsize=7.0];
compra -> fornecedor [headlabel="+ id", taillabel="+ fornecedor_id", arrowhead=odot, arrowtail=crow, fontname="Bitstream-Vera Sans", fontsize=7.0, dir=both];
compra_produto -> compra [headlabel="+ id", taillabel="+ compra_id", arrowhead=odot, arrowtail=crow, fontname="Bitstream-Vera Sans", fontsize=7.0, dir=both];
compra_produto -> produto [headlabel="+ id", taillabel="+ produto_id", arrowhead=odot, arrowtail=crow, fontname="Bitstream-Vera Sans", fontsize=7.0, dir=both];
}
